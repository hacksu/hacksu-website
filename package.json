{
  "name": "hacksu-2021",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "cross-env NODE_ENV=production node server/index.js",
    "dev": "nodemon --watch server --watch db --ignore \"db/*.json\" server/index.js",
    "build": "bunx vite-ssg build -c src/vite.config.js",
    "preview": "bunx --bun vite preview",
    "update": "git fetch && git pull && bun run build && pm2 restart hacksu"
  },
  "browser": {
    "markdown-it": false
  },
  "dependencies": {
    "@unhead/vue": "^1.11.20",
    "@vueuse/head": "^1.3.1",
    "body-parser": "^1.20.3",
    "cross-env": "^7.0.3",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "i": "^0.3.7",
    "markdown-it": "^13.0.2",
    "multer": "^1.4.5-lts.2",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "remult": "^0.22.12",
    "sharp": "^0.32.6",
    "vite-ssg": "^0.22.2",
    "vue": "^3.5.16",
    "vue-advanced-cropper": "^2.8.9",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.2",
    "nodemon": "^3.1.10",
    "sass": "^1.89.2",
    "vite": "^4.5.14"
  },
  "trustedDependencies": [
    "@parcel/watcher"
  ]
}
